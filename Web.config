<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <connectionStrings>
    <add name="constr" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=LeaveManagementDB;Integrated Security=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <system.web>
    <compilation targetFramework="4.7.2" />
    <sessionState mode="InProc" cookieless="false" timeout="20" />
    <pages>
      <controls>
        <add assembly="Microsoft.AspNet.Web.Optimization.WebForms" namespace="Microsoft.AspNet.Web.Optimization.WebForms" tagPrefix="webopt" />
      </controls>
    </pages>
  </system.web>
  <runtime>
    <!--
    Assembly binding redirects tell the runtime to use a specific version of an assembly,
    even if other referenced libraries expect older versions.
    This helps avoid conflicts and ensures consistent behavior across packages.
  -->
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <!--
      Redirect Microsoft.Web.Infrastructure to the correct version.
      This assembly is used internally by some ASP.NET features like optimization.
      Make sure the version here matches the installed DLL version (usually 1.0.0.0).
    -->
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Web.Infrastructure" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.0.0.0" newVersion="1.0.0.0" />
      </dependentAssembly>
      <!--
      Redirect WebGrease (used by bundling/minification) to the expected version.
    -->
      <dependentAssembly>
        <assemblyIdentity name="WebGrease" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.6.5135.21930" newVersion="1.6.5135.21930" />
      </dependentAssembly>
      <!--
      Redirect Antlr (used by WebGrease internally) to the latest installed version.
    -->
      <dependentAssembly>
        <assemblyIdentity name="Antlr3.Runtime" publicKeyToken="eb42632606e9261f" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.5.0.2" newVersion="3.5.0.2" />
      </dependentAssembly>
      <!--
      Redirect Newtonsoft.Json to ensure compatibility with other libraries.
      Widely used for JSON serialization/deserialization.
    -->
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-13.0.0.0" newVersion="13.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
